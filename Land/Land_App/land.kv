####################### LOGIN WINDOW ############################

<LoginWindow>:
	name: 'Login'

	email: email
	password: password

    canvas.before:
        Color: 
        	rgba: .94,1,.9, 1
        Rectangle:
        	pos: self.pos
        	size: self.size



    FloatLayout:

    	Label:
    		color: 1, 0, 0, 1
    		text: 'Beta version of land app by Ian Taylor; Please Note: This App is extremely underdeveloped.(V 0.0.1)'
    		font_size: 15
    		pos_hint: {'x': -.05, 'y': .47}

    	Label:
    		color: 1, 0, 0, 1
    		text: 'Email: '
    		font_size: 40
    		pos_hint: {'x':-.3, 'y':.4}
    	TextInput:
    		id: email
    		multiline: False
    		size_hint: (.5,.05)
    		pos_hint: {'x':.28, 'y':.87}
    	Label:
    		color: 1, 0, 0, 1
    		text: 'Password: '
    		font_size: 40
    		pos_hint: {'x':-.35, 'y':.3}
    	TextInput:
    		id: password
    		multiline: False
    		size_hint: (.5, .05)
    		pos_hint: {'x':.28, 'y':.765}
		Button:
			background_color: (.75, 0, .82, 1)
			pos_hint: {'x':.35, 'y':.55}
			size_hint:(.3, .15)
			text: 'Login'
			on_press: 
				root.loginbtn()
			on_release:
				app.root.current = 'ExploreLand' if root.loginbtn() == True else "Login"
		Label:
			color: 1,0,0,1
			text: 'Create An Account:'
			font_size: 40
			pos_hint: {'x':0, 'y':-.08}
		Button:
			background_color: .75, 0, .7, .9
			pos_hint: {'x':.35, 'y':.17}
			size_hint: (.3, .15)
			text: 'Create a Host Account'
			on_release:
				app.root.current = 'CreateHostAccount'

		Button:
			background_color: .75,0,.7,.9
			pos_hint: {'x':.35, 'y':.01}
			size_hint: (.3, .15)
			text: 'Create a User Account'
			on_release:
				app.root.current = 'CreateUserAccount'


